{
    "version": 2,
    "builds": [
        {
            "src": "nuxt.config.ts",
            "use": "@vercel/nuxt"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.html",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/html; charset=utf-8"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "s-maxage=1, stale-while-revalidate=59"
                }
            ]
        }
    ]
}